        app:layout_constraintTop_toBottomOf="@+id/tV_order_summary_payment_details_title"
        app:layout_constraintVertical_bias="0.054" />

    <TextView
        android:id="@+id/tV_order_summary_payment_method_selected_display"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/tV_order_summary_payment_method_selected_display"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.162"
        app:layout_constraintStart_toEndOf="@+id/tV_order_summary_payment_method_selected_title"
        app:layout_constraintTop_toBottomOf="@+id/tV_order_summary_payment_details_title"
        app:layout_constraintVertical_bias="0.059" />

    <TextView
        android:id="@+id/tv_order_summary_total_cost_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="13dp"
        android:text="@string/tv_order_summary_total_cost_title"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.225"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tV_order_summary_payment_method_selected_title"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/tv_order_summary_total_cost_display"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
